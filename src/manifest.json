{
  "manifest_version": 2,
  "name": "__MSG_ext_name__",
  "version": "1.15.5",
  "default_locale": "en",
  "description": "__MSG_ext_desc__",
  "icons": {
    "16": "assets/icons/favicon-16x16.ico",
    "96": "assets/icons/favicon-96x96.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "__MSG_ext_name__",
    "default_icon": {
      "16": "assets/icons/favicon-16x16.ico",
      "96": "assets/icons/favicon-96x96.png"
    },
    "default_popup": "index.html#/popup"
  },
  "options_page": "index.html#/app/options",
  "commands": {
    "store-selected-tabs": {
      "description": "__MSG_cmd_store_selected_tabs__"
    },
    "store-all-tabs": {
      "description": "__MSG_cmd_store_all_tabs__"
    },
    "restore-lastest-list": {
      "description": "__MSG_cmd_restore_lastest_list__"
    },
    "open-lists": {
      "description": "__MSG_cmd_open_lists__"
    },
    "store-all-in-all-windows": {
      "description": "__MSG_cmd_store_all_in_all_windows__"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search*",
        "https://www.google.com.hk/search*",
        "https://www.baidu.com/s*",
        "https://www.bing.com/search*",
        "https://duckduckgo.com/*",
        "https://www.sogou.com/web*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": [
    "<all_urls>",
    "commands",
    "contextMenus",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com__DEV_CSP_SCRIPT__; object-src 'self'; connect-src 'self'__DEV_CSP_CONNECT__",
  "web_accessible_resources": [
    "*"
  ]
}
