{
  "manifest_version": 3,
  "name": "__MSG_ext_name__",
  "version": "1.15.5",
  "default_locale": "en",
  "description": "__MSG_ext_desc__",
  "icons": {
    "16": "assets/icons/icon16.png",
    "32": "assets/icons/icon32.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "__MSG_ext_name__",
    "default_icon": {
      "16": "assets/icons/icon16.png",
      "32": "assets/icons/icon32.png",
      "48": "assets/icons/icon48.png"
    },
    "default_popup": "index.html#/popup"
  },
  "options_page": "index.html#/app/options",
  "commands": {
    "store-selected-tabs": {
      "description": "__MSG_cmd_store_selected_tabs__"
    },
    "store-all-tabs": {
      "description": "__MSG_cmd_store_all_tabs__"
    },
    "restore-lastest-list": {
      "description": "__MSG_cmd_restore_lastest_list__"
    },
    "open-lists": {
      "description": "__MSG_cmd_open_lists__"
    },
    "store-all-in-all-windows": {
      "description": "__MSG_cmd_store_all_in_all_windows__"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search*",
        "https://www.google.com.hk/search*",
        "https://www.baidu.com/s*",
        "https://www.bing.com/search*",
        "https://duckduckgo.com/*",
        "https://www.sogou.com/web*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": [
    "commands",
    "contextMenus",
    "storage",
    "tabs",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'; connect-src 'self' https://*.google.com https://*.googleapis.com https://sentry.io ws://localhost:8098 http://127.0.0.1:3000 ws://127.0.0.1:3000;"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}